{
	"name": "file-share",
	"version": "0.1.0",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "bun tailwind; bun run src/index.ts",
		"dev": "bun concurrently \"bun run --watch src/index.ts\" \"bun tailwind -w\"",
		"tailwind": "bun tailwindcss -i ./src/assets/main.css -o ./public/output.css"
	},
	"dependencies": {
		"@elysiajs/html": "^1.3.0",
		"@elysiajs/static": "^1.3.0",
		"@tailwindcss/cli": "^4.1.10",
		"@tqman/nice-logger": "^1.1.1",
		"elysia": "latest",
		"elysia-rate-limit": "^4.4.0",
		"tailwindcss": "^4.1.10"
	},
	"devDependencies": {
		"bun-types": "latest",
		"concurrently": "^9.1.2"
	},
	"module": "src/index.js"
}
